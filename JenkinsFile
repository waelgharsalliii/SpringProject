pipeline{
    agent any



    stages {


        stage('Getting project from Git') {
            steps{
      			checkout([$class: 'GitSCM', branches: [[name: '*/main']],
			extensions: [],
			userRemoteConfigs: [[url: 'https://github.com/waelgharsalliii/SpringProject.git']]])
            }
        }


       
        stage('SONARQUBE') {
            steps{

             		sh 'mvn clean verify sonar:sonar -Dsonar.projectKey=DevOps_Project -Dsonar.projectName='DevOps_Project' -Dsonar.host.url=http://192.168.1.22:9000 -Dsonar.token=sqp_5e92138024d3378c95b08b07443d753692edc27d'
            }
        }


        stage('Publish to Nexus') {
            steps {


  sh 'mvn deploy'


            }
        }



}
}









 
